{
	"name": "jsontodart",
	"displayName": "Json To Dart",
	"description": "Plain object to Dart Object",
	"version": "0.3.0",
	"publisher": "MuhammadNorzariman",
	"keywords": [
		"json",
		"dart",
		"flutter",
		"json to dart",
		"plain object"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/shiburagi/JsonToDart.git"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == dart",
					"command": "jsontodart.convertFromClipboardToFile",
					"group": "jsontodart@1"
				},
				{
					"when": "explorerResourceIsFolder",
					"command": "jsontodart.convertFromClipboardToFolder",
					"group": "jsontodart@1"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == dart",
					"command": "jsontodart.convertFromClipboardToFile",
					"group": "jsontodart@1"
				}
			]
		},
		"commands": [
			{
				"command": "jsontodart.convertFromClipboard",
				"title": "JsonToDart: Convert JSON from Clipboard"
			},
			{
				"command": "jsontodart.convertFromClipboardToFile",
				"title": "Convert JSON from Clipboard Here"
			},
			{
				"command": "jsontodart.convertFromClipboardToFolder",
				"title": "Convert JSON from Clipboard Here"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/camelcase": "^5.2.0",
		"@types/fs-extra": "^9.0.6",
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.167",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@types/yaml": "^1.9.7",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"fs-extra": "^9.0.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
